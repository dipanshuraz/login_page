{
  //   "env": {
  //     "browser": true,
  //     "es2021": true,
  //     "jest": true
  //   },
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier"
    // "prettier/react"
  ],
  "plugins": ["jsx-a11y", "prettier"],
  "rules": {
    "semi": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "error",
      {
        "semi": true
      }
    ]
    // "no-unused-vars": "warn",
    // "no-process-exit": "off",
    // "object-shorthand": "off",
  }
}

